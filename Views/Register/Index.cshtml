
@model NestedToDoApplication.Models.MASTERUSER
<head>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>


<div class="container text-center mt-5">
    <div class="row">
        <div class="col-md-4 offset-md-4">
            <div class="card rounded">
                <div class="card-body">
                    @using (Html.BeginForm("Index", "Register", FormMethod.Post, new { @class = "needs-validation", @novalidate = "novalidate" }))
                    {
                        <div class="form-row">
                            <div class="col-md-12 mb-3">
                                <label for="validationTooltip01">@Html.LabelFor(m => m.Username)</label><br>
                                @Html.TextBoxFor(m => m.Username, new { @class = "form-control" })<br>
                                @Html.ValidationMessageFor(m => m.Username, "", new { @class = "text-danger" })

                                <label for="validationTooltip02">@Html.LabelFor(m => m.LastName)</label><br>
                                @Html.TextBoxFor(m => m.LastName, new { @class = "form-control" })<br>

                                <label for="validationTooltip02">Password</label><br />
                                @Html.PasswordFor(m => m.UPassword, new { @class = "form-control", @id = "validationTooltip03" })
                                @Html.ValidationMessageFor(m => m.UPassword, "", new { @class = "text-danger" })
                                @ViewBag.ErrorMessage
                            </div>
                        </div>
                        <button class="btn btn-primary" type="submit">Submit form</button>
                    }
                </div>
            </div>
        </div>
    </div>
</div>

<style>
    body {
        background-color: #f0f8ff; /* Very light blue background color */
    }
</style>



